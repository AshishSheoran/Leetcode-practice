# Easy Level

/**
 * @param {number[]} nums
 * @return {number}
 */
var arraySign = function(nums) {
    let product = 1;
    
    for(let num of nums) {
        if (num < 0) {
                product *= -1;
            }
            else if (num == 0) {
                product = 0;
            }
    }
    
    return product;
    
};
