# EASY

/**
 * @param {number[]} arr
 * @return {boolean}
 */
var uniqueOccurrences = function(arr) {
    let map = new Map();
    
    for(let i of arr) {
        map.set(i, (map.get(i) || 0) +1);
    }
    
    let result = [];
    
    for(let value of map.values()) {
        result.push(value);
    }
    
    let unique = [...new Set(result)];
    
    return unique.length === result.length ? true : false;
};
